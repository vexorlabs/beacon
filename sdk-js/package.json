{
  "name": "beacon-sdk",
  "version": "0.1.0",
  "description": "Instrumentation SDK for Beacon â€” Chrome DevTools for AI Agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./integrations/openai": {
      "import": "./dist/integrations/openai.js",
      "types": "./dist/integrations/openai.d.ts"
    },
    "./integrations/anthropic": {
      "import": "./dist/integrations/anthropic.js",
      "types": "./dist/integrations/anthropic.d.ts"
    },
    "./integrations/vercel-ai": {
      "import": "./dist/integrations/vercel-ai.js",
      "types": "./dist/integrations/vercel-ai.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "openai": ">=4.0.0",
    "@anthropic-ai/sdk": ">=0.25.0",
    "ai": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "vitest": "^2.0.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
